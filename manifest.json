{
	"manifest_version": 2,

	"name": "Authors Who Are Douchenozzles",
    "short_name": "Douchenozzles",
	"description": "Adds Alleged Douchenozzle to Bad-acting Authors.",
	"author": "Skulleigh",
	"version": "1.1",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
